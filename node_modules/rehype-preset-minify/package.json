{
  "name": "rehype-preset-minify",
  "version": "6.0.0",
  "description": "rehype preset to minify and mangle HTML",
  "license": "MIT",
  "keywords": [
    "unified",
    "rehype",
    "rehype-plugin",
    "plugin",
    "html",
    "min",
    "minify",
